{
  "expo": {
    "name": "Turmas",
    "slug": "turmas-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "dark",
    "backgroundColor": "#09090A",
    "splash": {
      "backgroundColor": "#09090A",
      "resizeMode": "contain"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.turmasapp",
      "backgroundColor": "#09090A"
    },
    "android": {
      "package": "com.turmasapp",
      "backgroundColor": "#09090A",
      "adaptiveIcon": {
        "backgroundColor": "#09090A"
      }
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/favicon.png"
    }
  }
}
